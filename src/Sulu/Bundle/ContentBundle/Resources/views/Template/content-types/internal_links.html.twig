<div id="{{ id|raw }}"
     data-property='{{ property|json_encode }}'
     data-mapper-property="{{ property.name }}"
     data-form="true"
     data-type="internalLinks"
     data-type-instance-name="{{ id|raw }}"
     data-aura-component="internal-links@sulucontent"
     data-aura-instance-name="{{ id|raw }}"
     data-aura-url="/admin/api/nodes?language={{ languageCode }}{% if webspaceKey != '' %}&webspace={{ webspaceKey }}{% endif %}&flat=true"
     data-aura-result-key="nodes"
     data-aura-navigate-event="sulu.content.navigate"
     data-aura-webspace="{{ webspaceKey }}"
     data-aura-locale="{{ languageCode }}">
</div>
